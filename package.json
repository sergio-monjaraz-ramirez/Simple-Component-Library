{
  "name": "simple-component-library",
  "version": "1.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/simple-component-library.js",
      "require": "./dist/simple-component-library.cjs",
      "types": "./dist/simple-component-library.d.ts"
    }
  },
  "main": "dist/simple-component-library.cjs",
  "module": "dist/simple-component-library.js",
  "types": "dist/simple-component-library.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "update-version": "npm version patch --force",
    "artifactregistry-login": "npx google-artifactregistry-auth ./.npmrc",
    "publish-library": "npm run build && npm run update-version && npm publish --access=public"
  },
  "dependencies": {
    "charts.css": "^1.1.0",
    "vite-plugin-css-injected-by-js": "^3.5.1",
    "vue": "^3.4.37"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.9.0",
    "@storybook/addon-essentials": "^8.3.1",
    "@storybook/addon-interactions": "^8.3.1",
    "@storybook/addon-links": "^8.3.1",
    "@storybook/addon-onboarding": "^8.3.1",
    "@storybook/blocks": "^8.3.1",
    "@storybook/test": "^8.3.1",
    "@storybook/vue3": "^8.3.1",
    "@storybook/vue3-vite": "^8.3.1",
    "@types/node": "^22.5.5",
    "@vitejs/plugin-vue": "^5.1.2",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "storybook": "^8.3.1",
    "tailwindcss": "^3.4.11",
    "typescript": "^5.5.3",
    "vite": "^5.4.1",
    "vite-plugin-dts": "^4.2.1",
    "vite-plugin-lib-inject-css": "^2.1.1",
    "vue-tsc": "^2.0.29"
  },
  "description": "## Overview This is a library of custom components for reports generated by Report Worker. This library was built using Vue 3 and Tailwind.",
  "author": "",
  "license": "ISC"
}
